package testing;

import java.util.Scanner;

public class testing {

	public static void main(String[] args) {
		Scanner scanner = new Scanner(System.in);
		int value;
		value = scanner.nextInt();
		
		//90점 이상이면 1등급, 80점 이상, 90점 미만이면 2등급
		if(value >= 90) {
			System.out.println("1등급이다 아주 훌륭하다 이놈아");
		} else if(value >= 80 && value < 90) {
			System.out.println("2등급이다 차아암 잘했다 이놈아");
		} else if (value >= 70 && value < 80) {
			System.out.println("3등급이다 좋구나 이놈아");
		} else {
			System.out.println("4등급이다 분발해 새꺄");
		}
	
	
	}

}
