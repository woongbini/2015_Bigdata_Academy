package polymorphism;

public class Rectangle extends Geometry {
	private int width, height;
	
	public Rectangle (int x, int y, int width, int height) { 
		super(x, y);
		this.height = height;
		this.width = width;
	}
	public void draw() {
		System.out.println("좌표:"+x+","+y+" 가로세로 :"+width+","+height);
	}
}
